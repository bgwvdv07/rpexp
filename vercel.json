{
  "cleanUrls": true,
  "redirects": [
    {
      "source": "/blog",
      "destination": "https://reppards-blog.vercel.app",
      "permanent": true
    },
    {
      "source": "/blog/:slug*",
      "destination": "https://reppards-blog.vercel.app/:slug*",
      "permanent": true
    },
    {
      "source": "/old-about",
      "destination": "/about",
      "permanent": true
    }
  ]
}
