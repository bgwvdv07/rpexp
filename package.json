{
  "name": "reppard's_landscaping_home_page",
  "version": "1.0.0",
  "description": "express driven landing page",
  "main": "index.js",
  "engines": {
    "node": ">=16.17.1"
  },
  "private": true,
  "scripts": {
    "build:css": "purgecss --config purgecss.config.js && postcss ./public/dist/css/*.css -d ./public/dist/css",
    "build:js": "terser ./public/js/*.js -o ./public/dist/js/main.min.js -c -m",
    "build:copy": "mkdir -p public/dist && cp ./public/index.html ./public/dist/index.html && cp -r ./public/images ./public/dist/images",
    "build": "npm run build:css && npm run build:js && npm run build:copy",
    "start": "NODE_ENV=development node index.js",
    "start:prod": "NODE_ENV=production node index.js"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@netlify/functions": "^2.7.0",
    "@types/express": "^4.17.21",
    "body-parser": "^1.20.3",
    "compression": "^1.7.4",
    "connect": "^3.7.0",
    "cors": "^2.8.5",
    "css-aspect-ratio": "^1.0.5",
    "dotenv": "^16.4.5",
    "ejs": "^3.1.10",
    "express": "^4.19.2",
    "express-interceptor": "^1.2.0",
    "googleapis": "^160.0.0",
    "multer": "^1.4.5-lts.1",
    "node-cache": "^5.1.2",
    "node-fetch": "^3.3.2",
    "nodemailer": "^6.9.16",
    "sass-middleware": "^0.0.3",
    "serverless-http": "^3.2.0"
  },
  "devDependencies": {
    "cssnano": "^7.1.1",
    "postcss": "^8.5.6",
    "postcss-cli": "^11.0.1",
    "purgecss": "^7.0.2",
    "sass": "^1.93.0",
    "sharp": "^0.34.3",
    "terser": "^5.44.0"
  }
}
