{ "version": 2,

  "cleanUrls": true,

  "headers": [
    {
    "source": "/(.*)",
    "headers": [
      {
      "key": "accept-encoding",
      "value": "gzip"
    },
    {
    "source": "/(.*)",
    "headers": [
      {
        "key": "content-encoding",
        "value": "gzip"
      }
    ]
  }
    ]
}
  ]

}